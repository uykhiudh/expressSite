<div class="weatherWidget">
    {{#each partials.weatherData.locations}}
        <div class="location">
            <h3>{{name}}</h3>
            <a href="{{forecastUrl}}">
                <img width="30" src="{{iconUrl}}" alt="{{weather}}">
                {{weather}}, {{temp}}
            </a>
        </div>
    {{/each}}
    <small>Soruce: <a href="http://www.wunderground.com">Weather Underground</a></small>
</div>